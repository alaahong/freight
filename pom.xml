<?xml version="1.0" encoding="UTF-8"?>
<project xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xmlns="http://maven.apache.org/POM/4.0.0"
         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
    <modelVersion>4.0.0</modelVersion>

    <groupId>cn.ianzhang.sample</groupId>
    <artifactId>freight</artifactId>
    <packaging>pom</packaging>
    <version>1.0-SNAPSHOT</version>
    <modules>
        <module>freight_01</module>
        <module>freight_02</module>
        <module>freight_03</module>
        <module>freight_04</module>
        <module>freight_05</module>
        <module>freight_06</module>
        <module>freight_common</module>
